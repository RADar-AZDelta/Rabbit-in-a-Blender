CREATE OR REPLACE TABLE {{project_id}}.{{dataset_id_work}}.{{omop_table}}_upload AS
SELECT DISTINCT *
FROM (
    {%- for sql_file in sql_files -%}
        {%- if not loop.first %}
    UNION ALL
        {%- endif %}
    SELECT 
    {%- for column in columns -%}
        {%- if not loop.first -%}
            {{ ',' }}
        {%- endif %}
        {{ column }} 
    {%- endfor %}
    FROM {{project_id}}.{{dataset_id_work}}.{{omop_table}}__upload__{{sql_file}}
    {%- endfor %}
) t